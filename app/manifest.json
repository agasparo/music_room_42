{
	"manifest_version": 2,
	"name": "Music Room",
	"description": "Music Room is an app to listen and share music",
	"version": "1.0",
	"icons": {
		"128": "img/logo1.png"
	},
	"app": {
		"urls": [
			"http://lvh.me/music_room/"
		],
		"launch": {
			"web_url": "http://lvh.me/music_room/"
		},
		"background": {
      		"scripts": ["main.js"]
    	}
	},
	"permissions": [
		"unlimitedStorage",
		"notifications"
	]
}